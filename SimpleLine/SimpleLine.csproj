<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>SimpleLineLibrary</RootNamespace>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>SimpleLine</Title>
    <Authors>DanilKucherenko</Authors>
    <SignAssembly>False</SignAssembly>
    <AssemblyOriginatorKeyFile>SimpleLine</AssemblyOriginatorKeyFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Description>Library for impliment cli commnads in c# language</Description>
    <Copyright>@Hello world!</Copyright>
	<VersionPrefix>0.0.0.1</VersionPrefix>
	<VersionSuffix>alpha</VersionSuffix>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>embedded</DebugType>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>embedded</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Services\Finding\Exceptions\**" />
    <EmbeddedResource Remove="Services\Finding\Exceptions\**" />
    <None Remove="Services\Finding\Exceptions\**" />
  </ItemGroup>

</Project>