{
  "metadata": [
    {
      "src": [
        {
          "src": "../SimpleLine/",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [ "default", "modern" ],
    "globalMetadata": {
      "_appName": "SimpleLine",
      "_appTitle": "SimpleLine",
      "_enableSearch": true,
      "pdf": true
    }
  }
}